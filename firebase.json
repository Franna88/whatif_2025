{
    "functions": [
        {
            "source": "functions",
            "codebase": "default",
            "ignore": [
                "node_modules",
                ".git",
                "firebase-debug.log",
                "firebase-debug.*.log",
                "*.local"
            ],
            "predeploy": [
                "npm --prefix \"$RESOURCE_DIR\" run build"
            ]
        }
    ],
    "hosting": [
        {
            "target": "app",
            "public": "build/web",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "/panelbeaters-directory/:id/profile",
                    "function": "generateStaticProfile",
                    "region": "us-central1"
                },
                {
                    "source": "/sitemap.xml",
                    "function": "generateSitemap",
                    "region": "us-central1"
                },
                {
                    "source": "/render/**",
                    "function": "renderPage",
                    "region": "us-central1"
                },
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "cleanUrls": true,
            "appAssociation": "AUTO",
            "headers": [
                {
                    "source": "**/*.html",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "public, max-age=300, s-maxage=600"
                        },
                        {
                            "key": "X-Robots-Tag",
                            "value": "all"
                        },
                        {
                            "key": "google-site-verification",
                            "value": "YlOO70vPUqg7J4cW"
                        }
                    ]
                },
                {
                    "source": "**",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "no-cache, no-store, must-revalidate"
                        },
                        {
                            "key": "X-Robots-Tag",
                            "value": "all"
                        },
                        {
                            "key": "google-site-verification",
                            "value": "YlOO70vPUqg7J4cW"
                        }
                    ]
                }
            ]
        },
        {
            "target": "prerendered",
            "public": "build/web",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "cleanUrls": true,
            "appAssociation": "AUTO",
            "headers": [
                {
                    "source": "**/*.html",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "public, max-age=300, s-maxage=600"
                        },
                        {
                            "key": "X-Robots-Tag",
                            "value": "all"
                        },
                        {
                            "key": "google-site-verification",
                            "value": "YlOO70vPUqg7J4cW"
                        }
                    ]
                },
                {
                    "source": "**",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "no-cache, no-store, must-revalidate"
                        },
                        {
                            "key": "X-Robots-Tag",
                            "value": "all"
                        },
                        {
                            "key": "google-site-verification",
                            "value": "YlOO70vPUqg7J4cW"
                        }
                    ]
                }
            ]
        }
    ]
}